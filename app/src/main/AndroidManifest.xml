<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xq.mytime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".countdown.CountDownActivity1" />
        <activity android:name=".countdown.CountDownActivity2" />
        <activity android:name=".countdown.CountDownActivity3" />
        <activity android:name=".countdown.CountDownActivity4" />
        <activity android:name=".countdown.CountDownActivity5" />

        <activity android:name=".time.GetTimeActivity"/>
        <activity android:name=".time.GetTimeActivity1"/>
        <activity android:name=".time.GetTimeActivity2"/>
    </application>

</manifest>